{
	"name": "kbhagchandani/experian-php",
	"description": "PHP library for Experian API",
	"autoload": {
		"psr-4": {
			"Experian\\": "src/",
		}
	},
	"scripts": {
		"post-update-cmd": "Experian\\Request::installMozillaCACert",
		"post-package-install": [
			"Experian\\Request::installMozillaCACert"
		]
	},
	"require": {
		"php": ">=5.5",
		"guzzlehttp/guzzle": "~6.0",
		"doctrine/inflector":"~1.1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "4.0.*"
	}
}